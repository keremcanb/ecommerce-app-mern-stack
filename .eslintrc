{
  "env": {
    "commonjs": true,
    "es2020": true,
    "node": true
  },
  "extends": ["prettier", "airbnb-base"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "no-console": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "object-curly-newline": 0,
    "import/extensions": 0,
    "no-underscore-dangle": 0,
    "max-len": 0,
    "indent": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "none",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2
      }
    ]
  }
}
